{
  "name": "wechaty-chatopera",
  "version": "0.0.2",
  "description": "Chatopera Plugin for Wechaty, deliver chatbots in low-code way. <https://bot.chatopera.com>",
  "directories": {
    "doc": "docs",
    "example": "examples",
    "test": "tests"
  },
  "dependencies": {
    "@chatopera/sdk": "^2.5.5",
    "language-monitor": "^1.0.3",
    "wechaty-plugin-contrib": "^0.14.2",
    "wechaty-puppet-hostie": "^0.10.8"
  },
  "peerDependencies": {
    "wechaty-vorpal": "*"
  },
  "devDependencies": {
    "@chatie/eslint-config": "^0.12.1",
    "@chatie/git-scripts": "^0.6.2",
    "@chatie/semver": "^0.4.7",
    "@chatie/tsconfig": "^0.10.1",
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.6",
    "pkg-jq": "^0.2.4",
    "shx": "^0.3.2",
    "tstest": "^0.4.10",
    "wechaty": "^0.50.5",
    "wechaty-vorpal": "^0.6.11",
    "wechaty-puppet-mock": "^0.28.2"
  },
  "main": "dist/src/mod.js",
  "typings": "dist/src/mod.d.ts",
  "scripts": {
    "clean": "shx rm -fr dist/*",
    "dist": "npm run clean && tsc",
    "pack": "npm pack",
    "lint": "npm run lint:es && npm run lint:ts && npm run lint:md",
    "lint:md": "markdownlint README.md",
    "lint:ts": "tsc --noEmit",
    "example": "ts-node examples/ding-dong-bot.ts",
    "start": "npm run example",
    "test": "npm run lint && npm run test:unit",
    "test:pack": "bash -x scripts/npm-pack-testing.sh",
    "test:unit": "blue-tape -r ts-node/register \"src/**/*.spec.ts\" \"src/*.spec.ts\" \"tests/*.spec.ts\" \"tests/**/*.spec.ts\"",
    "lint:es": "eslint --ignore-pattern tests/fixtures/ '{bin,examples,scripts,src,tests}/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wechaty/wechaty-chatopera.git"
  },
  "keywords": [
    "wechaty",
    "plugin",
    "chatbot",
    "chatopera",
    "聊天机器人",
    "智能对话机器人"
  ],
  "author": "wj-Mat <wjmcater@gmail.com>",
  "contributors": [
    {
      "name": "Chatopera Dev",
      "email": "info@chatopera.com",
      "url": "https://www.chatopera.com"
    }
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/wechaty/wechaty-chatopera/issues"
  },
  "git": {
    "scripts": {
      "pre-push": "npx git-scripts-pre-push"
    }
  },
  "publishConfig": {
    "tag": "next"
  },
  "homepage": "https://github.com/wechaty/wechaty-chatopera#readme"
}